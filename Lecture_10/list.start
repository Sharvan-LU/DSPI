type list start
    number[*]

    function append is list
        arg w1 is list
        arg n is number
    start
        w1[len(w1)] -> n
        return w1
    end

    function remove is list
        arg w1 is list
        arg n is number
    start
        var i is number
        i -> 0

        var new is list
        new -> list()
        
        while i < len(w1) start
            if !(w1[i] == n) start
                new[i] -> w1[i]
            end
            
            i -> i+1
        end
        return new
    end
end

